#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main(){
    int n=6;
    vector<int> start={1,3,0,5,8,5};
    vector<int> finish={2,4,6,7,9,9};

    vector<pair<int, int>> activities;
    for(int i=0; i<start.size(); i++){
